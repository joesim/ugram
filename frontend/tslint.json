{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended", "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "jsx-no-multiline-js": false,
        "indent": false,
        "max-line-length": [true, 300],
        "jsx-no-lambda": false,
        "no-string-literal": false,
        "ordered-imports": false 
    },
    "rulesDirectory": []
}