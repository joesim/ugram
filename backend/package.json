{
  "name": "node-starter",
  "version": "1.0.0",
  "description": "Packs de départs pour développement avancé d'application web",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=test  babel-node src/index.js --presets es2015,stage-2",
    "pretest": "mongodb-runner start",
    "test": "NODE_ENV=test  mocha --require babel-core/register test/helper.js test/**/*.test.js",
    "posttest": "mongodb-runner stop",
    "coverage": "opn ./coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GLO3112/backend-starter-packs.git"
  },
  "babel": {
    "presets": [
      "stage-2",
      "es2015"
    ]
  },
  "author": "William Fortin & Vincent Séguin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GLO3112/backend-starter-packs/issues"
  },
  "homepage": "https://github.com/GLO3112/backend-starter-packs#readme",
  "dependencies": {
    "aws-sdk": "^2.6.15",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "dotenv": "^5.0.1",
    "ejs": "^2.5.2",
    "express": "^4.14.0",
    "lowdb": "^1.0.0",
    "mongodb": "^3.0.4",
    "mongoose": "^5.0.9",
    "morgan": "^1.7.0",
    "redis": "^2.6.3",
    "winston": "^2.3.0",
    "winston-cloudwatch": "^1.7.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^4.1.0",
    "mongodb-runner": "^3.6.1",
    "opn-cli": "^3.1.0",
    "sinon": "^4.4.3",
    "sinon-mongoose": "^2.0.2"
  }
}
